<script>
    function ButtonInjectChooseImage(id, img) {
        var url = "/FileManager?integration=buttoninject&output=" + encodeURI(id);
        if (img && img != "") {
            url += "&imgshow=" + encodeURI(img);
        }

        window.open(url, "MsgWindow", "width=830,height=600");
    }
</script>